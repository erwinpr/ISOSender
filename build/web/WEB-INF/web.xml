<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RepeatServlet</servlet-name>
        <servlet-class>RepeatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagingServlet</servlet-name>
        <servlet-class>PagingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestConvertServlet</servlet-name>
        <servlet-class>TestConvertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ISOSenderServlet</servlet-name>
        <servlet-class>ISOSenderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WalletServlet</servlet-name>
        <servlet-class>WalletServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RepeatServlet</servlet-name>
        <url-pattern>/RepeatServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PagingServlet</servlet-name>
        <url-pattern>/PagingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestConvertServlet</servlet-name>
        <url-pattern>/TestConvertServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ISOSenderServlet</servlet-name>
        <url-pattern>/ISOSenderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WalletServlet</servlet-name>
        <url-pattern>/WalletServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
